{
  "manifest_version": 3,
  "name": "Disliko",
  "version": "1.0.0",
  "description": "Bring the dislikes back to YouTube.",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Disliko"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
